{
  "appId": "com.jevon.pocket8",
  "appName": "Pocket8",
  "webDir": "dist",
  "ios": {
    "includePlugins": [
      "@boengli/capacitor-fullscreen",
      "@capacitor/action-sheet",
      "@capacitor/app",
      "@capacitor/dialog",
      "@capacitor/filesystem",
      "@capacitor/haptics",
      "@capacitor/share",
      "@capawesome/capacitor-file-picker"
    ]
  },
  "server": {
    "allowNavigation": [
      "corsproxy.io",
      "lexaloffle.com",
      "www.lexaloffle.com",
      "api.allorigins.win"
    ]
  },
  "plugins": {
    "CapacitorCookies": {
      "enabled": true
    },
    "CapacitorHttp": {
      "enabled": true
    },
    "Console": {
      "visibility": "visible"
    }
  },
  "loggingBehavior": "debug"
}
